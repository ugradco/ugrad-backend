{
  "name": "ugrad-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "clean": "rm -rf build && mkdir build"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.11.19",
    "nodemailer": "^6.5.0",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.1",
    "passport-2fa-totp": "^0.0.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
